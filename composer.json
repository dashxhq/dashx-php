{
    "name": "dashx/php",
    "description": "DashX SDK for PHP",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.4",
        "ramsey/uuid": "^4.7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dashx\\Php\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashx\\Php\\Laravel\\DashxServiceProvider"
            ],
            "aliases": {
                "DashX": "Dashx\\Php\\Laravel\\DashxFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dashx\\Php\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
